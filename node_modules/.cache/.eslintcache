\n        if (ref.current) {\n            observer.observe(ref.current);\n        }\n\n        // Cleanup function: Stops observing the element when the component unmounts.\n        return () => {\n            if (ref.current) {\n                observer.unobserve(ref.current);\n            }\n        };\n    }, [ref, hasBeenInView, threshold]); // Added hasBeenInView to the dependency array.\n\n    return hasBeenInView; // Return whether the element has been in view at least once.\n};\n\nexport default useInView;\n\n","/Users/home/Desktop/Workspace/HTML-CSS-JS-Projects/Kevin-Cendana-Website/src/components/Home/Home.js",["269"],[],"//--------------------------------------------------------------------------------------//\n//                                         Home                                         //\n//--------------------------------------------------------------------------------------//\n// Credit for rotating text animation: https://www.npmjs.com/package/react-rotating-text?activeTab=readme\nimport ReactRotatingText from './RotatingText';\nimport React, { useState, useEffect } from 'react';\nimport './Home.css';\nimport '../../normalize.css';\nfunction Home() {\n\n    // Trigger animations when the page loads\n    useEffect(() => {\n        const imageContainer = document.querySelector('.home-image-container');\n        imageContainer.classList.add('start-animation');\n    }, []);\n    \n    return (\n        <section className = \"home \" id = \"home\">\n            <div className=\"home__left\">\n            <div className=\"home__text-container\">\n                <div className=\"block-wrapper\">\n                    <h1 className=\"home__text-name\">Kevin Cendana</h1>\n                    <p className=\"home__text-alias\">\n                        <ReactRotatingText items={[\n                            'Computer Science Student',\n                            'Web Developer',\n                            'Software Engineer']} />\n               